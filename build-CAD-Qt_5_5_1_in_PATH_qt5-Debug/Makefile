####### Compile

main.o: ../CAD/main.cpp ../CAD/mainwindow.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o ../CAD/main.cpp

mainwindow.o: ../CAD/mainwindow.cpp ../CAD/mainwindow.h \
		ui_mainwindow.h \
		../CAD/mainview.h \
		../CAD/controlnet.h \
		../CAD/mesh.h \
		../CAD/vertex.h \
		../CAD/face.h \
		../CAD/halfedge.h \
		../CAD/objfile.h \
		../CAD/mousehandler.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o mainwindow.o ../CAD/mainwindow.cpp

mainview.o: ../CAD/mainview.cpp ../CAD/mainview.h \
		../CAD/controlnet.h \
		../CAD/mesh.h \
		../CAD/vertex.h \
		../CAD/face.h \
		../CAD/halfedge.h \
		../CAD/objfile.h \
		../CAD/mousehandler.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o mainview.o ../CAD/mainview.cpp

controlnet.o: ../CAD/controlnet.cpp ../CAD/controlnet.h \
		../CAD/mesh.h \
		../CAD/vertex.h \
		../CAD/face.h \
		../CAD/halfedge.h \
		../CAD/objfile.h
